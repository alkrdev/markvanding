model Machine {
    id              Int @id
    pumpname        String?
    time            DateTime?
    active          Boolean
    maintenances    Maintenance[]
    nozzle          String?
    model           String?

}

model Pump {
    id              Int @id @default(autoincrement())
    name            String @unique
    number          String @unique
    active          Boolean?
    startcode       String?
    stopcode        String?
}

model Maintenance {
    id              Int @id @default(autoincrement())
    machine         Machine? @relation(fields: [machineId], references: [id])
    machineId       Int?
    time            DateTime?
    note            String?
}